{
    "name": "kregel/code-generator",
    "description": "The an opinionated code generator.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "github@austinkregel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.3",
        "illuminate/database": "^v9.48",
        "illuminate/support": "^9.48",
        "laravel-zero/framework": "^v9.2",
        "laravel/pint": "^1.4",
        "nette/php-generator": "^3.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "illuminate/auth": "^9.48",
        "mockery/mockery": "^1.4.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["application"]
}
